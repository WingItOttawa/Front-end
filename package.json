{
  "name": "front-end",
  "version": "0.1.0",
  "private": true,
  "homepage": "https://WingItOttawa.github.io/Front-end",
  "dependencies": {
    "concurrently": "^3.6.0",
    "gh-pages": "^1.1.0",
    "react": "^16.4.1",
    "react-dom": "^16.4.1",
    "react-scripts": "1.1.4"
  },
  "scripts": {
    "predeploy": "yarn run build",
    "deploy": "gh-pages -d build",
    "start": "concurrently \"react-scripts start\"  \"sass --watch ./src/style/scss:./src/style/css\" ",
    "sass:build": "sass --update src/style/scss:src/style/css",
    "build": "yarn sass:build && react-scripts build",
    "test": "react-scripts test --env=jsdom",
    "eject": "react-scripts eject",
    "sass": "sass --watch src/style/scss:src/style/css"
  }
}
